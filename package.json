    "build:watch": "rimraf lib && concurrently \"npm run build:src:watch\" \"npm run build:types:watch\"",
    "build:src": "swc src --out-dir lib",
    "build:src:watch": "npm run build:types -- --watch",
    "build:types": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --collectCoverage",
    "coverage-start": "jest --collectCoverage && start coverage/lcov-report/index.html",
    "eslint": "eslint src",
    "eslint:fix": "eslint --fix src",
    "prettier:check": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "prepare": "npm test && npm run build"
